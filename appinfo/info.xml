    <id>overleaf_nextcloud</id>
    <name>Overleaf Nextcloud Integration</name>
    <version>1.1.0</version>

    <namespace>Overleaf</namespace>

    <licence>agpl</licence>
    <author mail="info@uni-muenster.de" homepage="https://www.uni-muenster.de">University of Muenster</author>

    <category>integration</category>
    <summary>Integration of Overleaf</summary>
    <description><![CDATA[This app integrates Overleaf into Nextcloud.]]></description>
    <bugs>https://www.uni-muenster.de</bugs>

    <dependencies>
        <nextcloud min-version="26" max-version="29"/>
    </dependencies>

    <navigations>
        <navigation>
            <name>Overleaf</name>
            <route>overleaf_nextcloud.launch.launch</route>
        </navigation>
    </navigations>

    <settings>
        <admin>OCA\Overleaf\Settings\AppSettings</admin>
        <admin-section>OCA\Overleaf\Settings\AppSettingsSection</admin-section>
    </settings>
</info>
